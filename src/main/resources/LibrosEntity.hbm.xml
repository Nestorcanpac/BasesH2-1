<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.LibrosEntity" table="LIBROS" schema="PUBLIC" catalog="BIBLIOTECA">
        <id name="id">
            <column name="ID" sql-type="integer"/>
        </id>
        <property name="titol">
            <column name="TITOL" sql-type="character varying(60)" length="60" not-null="true"/>
        </property>
        <property name="autor">
            <column name="AUTOR" sql-type="character varying(60)" length="60" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>